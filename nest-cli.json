{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/raram-queues/src",
  "monorepo": true,
  "root": "apps/raram-queues",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/raram-queues/tsconfig.app.json"
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "raram-queues": {
      "type": "application",
      "root": "apps/raram-queues",
      "entryFile": "main",
      "sourceRoot": "apps/raram-queues/src",
      "compilerOptions": {
        "tsConfigPath": "apps/raram-queues/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "riot-api": {
      "type": "library",
      "root": "libs/riot-api",
      "entryFile": "index",
      "sourceRoot": "libs/riot-api/src",
      "compilerOptions": {
        "tsConfigPath": "libs/riot-api/tsconfig.lib.json"
      }
    },
    "raram-analysis": {
      "type": "application",
      "root": "apps/raram-analysis",
      "entryFile": "main",
      "sourceRoot": "apps/raram-analysis/src",
      "compilerOptions": {
        "tsConfigPath": "apps/raram-analysis/tsconfig.app.json"
      }
    },
    "raram-stats": {
      "type": "application",
      "root": "apps/raram-stats",
      "entryFile": "main",
      "sourceRoot": "apps/raram-stats/src",
      "compilerOptions": {
        "tsConfigPath": "apps/raram-stats/tsconfig.app.json"
      }
    }
  }
}
